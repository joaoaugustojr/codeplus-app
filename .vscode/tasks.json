    "version": "2.0.0",
    "tasks": [
        {
            "label": "run-android",
            "type": "shell",
            "command": "react-native run-android"
        },
        {
            "label": "run-docker",
            "type": "shell",
            "command": "docker-compose up -d nginx mysql phpmyadmin redis workspace"
        },
        {
            "label": "run-docker-laravel",
            "type": "shell",
            "command": "docker-compose exec workspace bash"
        },
        {
            "label": "run-docker-mysql",
            "type": "shell",
            "command": "docker-compose exec mysql bash"
        },
        {
            "label": "run-docker-restart",
            "type": "shell",
            "command": "docker-compose restart"
        }
    ]
}

